<% elements = collections + works %>
<% elements.sort_by { |element| element.position } %>


<% elements.each do |element| %>
  <% if element.class.to_s =~ /Collection/ %>
    <%= render 'shared/works/collection_tile', collection: element %>
  <% end %>


  <% if element.class.to_s =~ /Work/ %>
    <%= render 'shared/works/work_tile', work: element %>
  <% end %>
<% end %>
